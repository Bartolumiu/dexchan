{
    "name": "dex-chan",
    "version": "0.0.6",
    "description": "Discord bot for the MangaDex ES server.",
    "main": "src/index.js",
    "scripts": {
        "test": "node --no-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
        "update": "npm update --save"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "testMatch": [
            "<rootDir>/tests/**/*.test.js"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "<rootDir>/src/**/*.js"
        ],
        "coverageDirectory": "<rootDir>/coverage",
        "coverageReporters": [
            "lcov",
            "text",
            "json-summary"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 0,
                "functions": 0,
                "lines": 0,
                "statements": 0
            }
        }
    },
    "keywords": [
        "discord",
        "bot",
        "mangadex"
    ],
    "author": "Bartolumiu",
    "license": "GPL-3.0",
    "dependencies": {
        "@discordjs/rest": "^2.4.0",
        "axios": "^1.7.7",
        "chalk": "^5.3.0",
        "discord.js": "^14.16.3",
        "dotenv": "^16.4.5",
        "mongoose": "^8.8.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nock": "^13.5.5"
    }
}
