{
    "name": "dex-chan",
    "version": "0.0.6",
    "description": "Discord bot for the MangaDex ES server.",
    "main": "src/index.js",
    "scripts": {
        "test": "nyc jest",
        "coverage": "nyc report --reporter=text --reporter=html",
        "update": "npm update --save"
    },
    "jest": {
        "testMatch": [
            "<rootDir>/tests/**/*.test.js"
        ]
    },
    "nyc": {
        "include": ["src/**/*.js"],
        "exclude": ["tests/**/*.test.js"],
        "reporter": ["text", "html", "lcov"],
        "all": true,
        "check-coverage": true,
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
    },
    "keywords": [
        "discord",
        "bot",
        "mangadex"
    ],
    "author": "Bartolumiu",
    "license": "GPL-3.0",
    "dependencies": {
        "@discordjs/rest": "^2.4.0",
        "axios": "^1.7.7",
        "chalk": "^5.3.0",
        "discord.js": "^14.16.3",
        "dotenv": "^16.4.5",
        "mongoose": "^8.8.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nock": "^13.5.5",
        "nyc": "^17.1.0"
    }
}
